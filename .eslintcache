rent.value).then(r => {\r\n            if (r){\r\n                props.getUid(r.user.uid)\r\n                firebase.auth().setPersistence(firebase.auth.Auth.Persistence.LOCAL)\r\n                props.setRedirect('/workspace')\r\n            }\r\n        })\r\n    }\r\n    return (\r\n        <div>\r\n            <form action=\"\" onSubmit={(e) => submit(e)}>\r\n                <div><input ref={login} name='login' type=\"text\" placeholder='login' /></div>\r\n                <div><input ref={password} name=\"password\" type=\"text\"  placeholder='password'/></div>\r\n\r\n                <button>Создать</button>\r\n                <div><em><NavLink to={'/login'}>Вернуться</NavLink></em></div>\r\n            </form>\r\n        </div>\r\n    );\r\n};\r\nconst mapDispatchToProps = {\r\n    getUid\r\n}\r\nexport default connect(null, mapDispatchToProps)(SignUp)\r\n",["207","208"],"E:\\notes-list\\src\\components\\Workspace.js",["209"],"import React, {useRef} from 'react';\r\nimport classes from './Workspace.module.css'\r\n\r\nimport { connect } from \"react-redux\";\r\nimport {activeNav, createNote} from \"../redux/actions/actions\";\r\nimport {firebase} from \"../Firebase\";\r\n\r\nconst Workspace = (props) => {\r\n    const input = useRef()\r\n    const clickHandler = (type) =>{\r\n        switch (type){\r\n            case 'create':\r\n                props.activeNav('create')\r\n                break\r\n        }\r\n    }\r\n    const submit = e =>{\r\n        e.preventDefault()\r\n        props.createNote(input.current.value)\r\n\r\n    }\r\n    return (\r\n        <div>\r\n            <div className={classes.wrapper}>\r\n                <div className={classes.noteList}>\r\n                    <h5>Notes List</h5>\r\n                    <button onClick={() => clickHandler('create')}>New Note</button>\r\n                </div>\r\n                <div className={classes.createNote}>\r\n                    <h3>Create Note</h3>\r\n                    <form action=\"\